<div class="card mt-4">
  <div class="card-body">
    <h5 class="card-title">
      <a href="{{ path('property.detail', {slug: property.slug, id: property.id} ) }}">{{property.title}}</a>
    </h5>
    <b class="card-text">{{property.city}} ({{property.postcode}})</b>
    <p class="card-text">
      {{property.rooms}} pieces - {{property.surface}}m²</br>
      <span class="text-primary">{{property.formattedPrice}} €</span>
    </p>
  </div>
</div>